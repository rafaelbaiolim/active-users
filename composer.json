{
  "name": "rlb/active-users",
  "description": "A Laravel Package for easily retrieving a list of active users and guests that are currently online.",
  "keywords": [
    "laravel",
    "php",
    "active uesrs",
    "online activity"
  ],
  "homepage": "https://github.com/joshrainwater/active-users",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Rafael Baiolim",
      "email": "rafaelbaiolim@gmail.com"
    },
    {
      "name": "Marcelo Sena",
      "email": "marcelosenaonline@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "~5.6|~5.7|~5.8|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "orchestra/testbench": "^4.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Rainwater\\Active\\": "src/Active"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rainwater\\Active\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rainwater\\Active\\ActiveServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
},
